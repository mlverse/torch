Package: torch
Type: Package
Title: Tensors and Neural Networks with 'GPU' Acceleration
Version: 0.11.0
Authors@R: c(
    person("Daniel", "Falbel", email = "daniel@rstudio.com", role = c("aut", "cre", "cph")),
    person("Javier", "Luraschi", email = "jluraschi@gmail.com", role = c("aut")),
